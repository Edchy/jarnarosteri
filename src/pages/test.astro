  /* Zigzag Circle Styling */
  .zigzag-circle {
    position: relative;
    width: 200px;
    height: 200px;
    border-radius: 50%;
    border: 10px solid transparent; /* Initial border to define size */
    /* Applying the zigzag border using border-image */
